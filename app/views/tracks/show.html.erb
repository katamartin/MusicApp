<%= @track.title %>
<br>
<%= up_one_level(@track) %>
<br>

<h3>Lyrics</h3>
<pre><%= ugly_lyrics(@track.lyrics) %></pre><br>
<a href="<%= edit_track_url(@track) %>">Edit</a>
<br>
<form action="<%= track_url(@track) %>" method="POST">
  <%= authenticate %>
  <input type="hidden" name="_method" value="DELETE">
  <input type="submit" value="Delete">
</form>
<br>
<%= render "notes/form", note: @note%>
<%= render partial: "notes/note", collection: @track.notes %>
<br>
